<script language="JavaScript" type="text/javascript">
	#if ($LGS-CUSTOMER-userAuthenticated)
		var loginTest = true;
	#else
		var loginTest = false;
	#end

var field = 'p_p_id'; 
var url = window.location.href; 
var urlPath=window.location.pathname;
if (urlPath.indexOf('/web/global-services/login') == 0){
if(url.indexOf('?' + field + '=') == -1){
window.location.replace( '/web/global-services/login?p_p_id=58&p_p_lifecycle=0&_58_redirect=%2Fgroup%2Fglobal-services%2F' );
}
if(url.indexOf('_58_redirect=') > 0 && loginTest){
window.location.replace( '/group/global-services' );
}
}

var allPortlets = "";
