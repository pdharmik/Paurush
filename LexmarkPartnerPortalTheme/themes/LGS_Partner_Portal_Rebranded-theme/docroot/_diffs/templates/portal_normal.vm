function markDirectCall(targetSystem){
	var userLocale = "$themeDisplay.getLocale()";
	var mdHost = "$LGS-CUSTOMER-markDirectHost";
	var userCountry = "$LGS-CUSTOMER-ldapUserCountry";
	var localeFirst = userLocale.substr(0,2);
	var localeLast = userLocale.substr(3);
	var fullURL = mdHost + targetSystem + "&ctry=" + localeLast + "&lang=" + localeFirst;
	window.open(fullURL,'_blank');
}

function callOmnitureAction(portlet, type){
	var s=s_gi('%omniture.account%'); 
	s.linkTrackVars='prop1,eVar1,prop6,eVar6,events'; 
	s.linkTrackEvents='event1'; 
	s.prop1=portlet; 
	s.eVar1=portlet; 
	s.prop6=type; 
	s.eVar6=type; 
	s.events='event1'; 
	s.tl(this,'o',type);
}
function callOmnitureCreateClaimAction(){
	var s=s_gi('%omniture.account%'); 
	s.linkTrackVars='events'; 
	s.events='event2'; 
	s.t();
}
function callOmnitureCompleteClaimAction(elapsedTime){
	var s=s_gi('%omniture.account%'); 
	s.linkTrackVars='prop7,eVar7,products,events'; 
	s.linkTrackEvents='event3,event4';
	s.prop7=elapsedTime; 
	s.eVar7=elapsedTime; 
	s.events='event3,event4'; 
	s.products=";;;;event4=" + elapsedTime;
	s.t();
}
function callOmnitureFieldErrorAction(fieldName){
    var s=s_gi('%omniture.account%'); 
    s.linkTrackVars='prop8,eVar8'; 
    s.prop8=fieldName;
    s.eVar8=fieldName;  
    s.t();
}
function callOmnitureReportRunAction(reportName){
    var s=s_gi('%omniture.account%'); 
    s.linkTrackVars='prop9,eVar9'; 
    s.prop9=reportName;
    s.eVar9=reportName;  
    s.t();
}

