	var s=s_gi('lxmpartnerqa'); 
	s.linkTrackVars='prop7,eVar7,products,events'; 
	s.linkTrackEvents='event3,event4';
	s.prop7=elapsedTime; 
	s.eVar7=elapsedTime; 
	s.events='event3,event4'; 
	s.products=";;;;event4=" + elapsedTime;
	s.t();
}
function callOmnitureFieldErrorAction(fieldName){
