<script type="text/javascript">
function autoResizeDiv()
{
       var min_height=window.innerHeight;
       if ( $( "#banner" ).length ) {
               min_height=min_height-$("#banner").height();
       }
       if ( $( "#topnavigation" ).length ) {
               min_height=min_height-$("#topnavigation").height();
       }  
       if ( $( "#footer" ).length ) {
               min_height=min_height-$("#footer").height();
       }                	   
       $("#content").css({"min-height":(min_height)+"px"});
}
window.onresize =autoResizeDiv;
autoResizeDiv();
</script>

