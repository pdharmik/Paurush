function addPortlet(portletSingle) {
allPortlets = allPortlets + "," + portletSingle;


}
function markDirectCall(targetSystem){
	var userLocale = "$themeDisplay.getLocale()";
	var mdHost = "$LGS-CUSTOMER-markDirectHost";
	var userCountry = "$LGS-CUSTOMER-ldapUserCountry";
	var localeFirst = userLocale.substr(0,2);
	var localeLast = userLocale.substr(3);
	var fullURL = mdHost + targetSystem + "&ctry=" + localeLast + "&lang=" + localeFirst;
	window.open(fullURL,'_blank');
}

function callOmnitureAction(portlet, type){
	var s=s_gi('lxmpartnerqa'); 
	s.linkTrackVars='prop1,eVar1,prop6,eVar6,events'; 
	s.linkTrackEvents='event1'; 
	s.prop1=portlet; 
	s.eVar1=portlet; 
	s.prop6=type; 
	s.eVar6=type; 
	s.events='event1'; 
	s.tl(this,'o',type);
}
function callOmnitureCreateClaimAction(){
	var s=s_gi('lxmpartnerqa'); 
	s.linkTrackVars='events'; 
	s.events='event2'; 
	s.t();
}
