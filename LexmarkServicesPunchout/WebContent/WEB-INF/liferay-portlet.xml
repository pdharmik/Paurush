		<requires-namespaced-parameters>false</requires-namespaced-parameters>
		<render-weight>1</render-weight>
		<header-portlet-javascript>/js/jQuery/jquery.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jQuery/jquery-ui.min.js</header-portlet-javascript>
	</portlet>
