<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="GlobalContactDO" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>Siebel BO</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>Account</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>Account Note</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
              
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>id</value></key>
	         		<value>Id</value>
				</entry>
				<entry>
	         		<key><value>noteType</value></key>
	         		<value>Note Type</value>
				</entry>
				<entry>
	         		<key><value>note</value></key>
	         		<value>Note</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel1AccountId</value></key>
	         		<value>LXK Account Global DUNS Number</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel2AccountId</value></key>
	         		<value>LXK Account Domestic DUNS Number</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel3AccountId</value></key>
	         		<value>LXK MDM Legal Entity ID #</value>
				</entry>
				<entry>
	         		<key><value>accountLevel</value></key>
	         		<value>LXK SW MDM Account Level</value> 
				</entry>
	     	</map>
		</property>
	   	     	  
    </bean>  
</beans>