<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="AcceptOrderLineItemDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK MPS Ord Inbound</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK C Order Entry - Line Items (EAI)</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
              
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>
	     		<entry>
	         		<key><value>id</value></key>
	         		<value>Id</value>
				</entry>
				<entry>
	         		<key><value>status</value></key>
	         		<value>Status</value>
				</entry>
				<entry>
	         		<key><value>vendorId</value></key>
	         		<value>LXK MPS Service Provider Id</value>
				</entry>
				<entry>
        			<key><value>orderHeaderId</value></key>
       				<value>Order Id</value>
				</entry>
	     	</map>
		</property>
    </bean>  
</beans>
