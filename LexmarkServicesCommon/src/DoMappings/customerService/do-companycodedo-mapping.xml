<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="CompanyCodeDo" class="com.amind.data.service.DomainObjMapping">
  
    <!-- data source info -->
       <property name="dataSourceName">
            <value>EAI Siebel Adapter</value>
        </property>

       <property name="dsTargetObjectName">
            <value>LXK SAP Account Detail IO</value>
        </property>
        
        <property name="dsTargetComponentName">
            <value>LXK Company Codes</value>
        </property>

        <property name="packageName">
            <value>com.lexmark.service.impl.real.domain</value>
        </property>
             
        <!-- field mapping --> 
        <property name="doFieldNameToDsFieldNameMap">
            <map>
                <entry>
                    <key><value>companyDescription</value></key>
                    <value>LXK Company Description</value>
                </entry> 
                 <entry>
                    <key><value>value</value></key>
                    <value>LXK Value</value>
                </entry>                		
            </map>
        </property>
    </bean>  
</beans>
