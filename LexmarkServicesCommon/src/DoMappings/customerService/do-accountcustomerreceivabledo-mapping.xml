<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean id="AccountCustomerReceivableDo" class="com.amind.data.service.DomainObjMapping">
  
    <!-- data source info -->
       <property name="dataSourceName">
            <value>EAI Siebel Adapter</value>
        </property>

       <property name="dsTargetObjectName">
            <value>LXK MPS SAP Account IO</value>
        </property>
        
        <property name="dsTargetComponentName">
            <value>LXK SW Account</value>
        </property>

        <property name="packageName">
            <value>com.lexmark.service.impl.real.domain</value>
        </property>
             
        <!-- field mapping --> 
        <property name="doFieldNameToDsFieldNameMap">
            <map>
                <entry>
                    <key><value>id</value></key>
                    <value>Id</value>
                </entry>
                <entry>
	         		<key><value>customerSapAccount</value></key>
	         		<value>com.lexmark.service.impl.real.domain.CustomerSapAccountDo</value>
				</entry>	
				<entry>
	         		<key><value>accountName</value></key>
	         		<value>Name</value>
				</entry>	
            </map>
        </property>
    </bean>  
</beans>
