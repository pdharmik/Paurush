<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="ListOfValues" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK SW List of Values</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK SW List of Values</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
              
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>id</value></key>
	         		<value>Id</value>
				</entry>
				<entry>
	         		<key><value>type</value></key>
	         		<value>Type</value>
				</entry>
				<entry>
	         		<key><value>value</value></key>
	         		<value>Value</value>
				</entry>
				<entry>
	         		<key><value>name</value></key>
	         		<value>Name</value>
				</entry>
				<entry>
	         		<key><value>language</value></key>
	         		<value>Language</value>
				</entry>
				<entry>
	         		<key><value>languageName</value></key>
	         		<value>Language Name</value>
				</entry>
				<entry>
	         		<key><value>active</value></key>
	         		<value>Active</value>
				</entry>
				<entry>
	         		<key><value>low</value></key>
	         		<value>Low</value>
				</entry>
				<entry>
	         		<key><value>parentName</value></key>
	         		<value>Parent</value>
				</entry>
				<entry>
	         		<key><value>description</value></key>
	         		<value>Description</value>
				</entry>
				
	     	</map>
		</property>
	   	     	  
    </bean>  
</beans>