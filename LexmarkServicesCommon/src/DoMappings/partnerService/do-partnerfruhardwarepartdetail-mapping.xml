<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="PartnerFruHardwarePartDetailDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
   	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       <property name="dsTargetObjectName">
			<value>LXK SW Parts Detail Partner Portal</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK SW Part List - Partner Portal</value>
        </property>
        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
             
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>modelNumber</value></key>
	         		<value>LXK Machine Type Model</value>
				</entry>
				<entry>
	         		<key><value>id</value></key>
	         		<value>LXK R Product Id</value>
				</entry>		
				<entry>
	         		<key><value>partNumber</value></key>
	         		<value>Part Name</value>
				</entry>
				<entry>
	         		<key><value>partName</value></key>
	         		<value>Part Number</value>
				</entry>
				<entry>
	         		<key><value>partLocations</value></key>
	         		<value>com.lexmark.service.impl.real.domain.PartnerHardwarePartDetailLocations</value>
				</entry>
	    	</map>
		</property>
    </bean>  
</beans>