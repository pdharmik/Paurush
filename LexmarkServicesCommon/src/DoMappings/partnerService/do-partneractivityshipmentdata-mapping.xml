<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="PartnerActivityShipmentDataDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
   	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       <property name="dsTargetObjectName">
			<value>LXK SW Request Detail Partner Portal</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK MPS Shipment Item</value>
        </property>
        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
             
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>shipmentCarrier</value></key>
	         		<value>LXK MPS Carrier</value>
				</entry> 	
				<entry>
	         		<key><value>shipmentTrackingNumber</value></key>
	         		<value>LXK MPS Tracking #</value>
				</entry> 	
				<entry>
	         		<key><value>shipmentTrackingStatus</value></key>
	         		<value>LXK MPS Tracking Status</value>
				</entry> 
				<entry>
	         		<key><value>shipmentDate</value></key>
	         		<value>Shipment Date</value>
				</entry> 
			</map>
		</property>
    </bean>  
</beans>