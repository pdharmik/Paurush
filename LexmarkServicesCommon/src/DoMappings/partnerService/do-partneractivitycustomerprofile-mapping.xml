<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="PartnerActivityCustomerProfileDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
   	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       <property name="dsTargetObjectName">
			<value>LXK SW Request Detail Partner Portal</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK MPS Profile Details</value>
        </property>
        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
             
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>fieldName</value></key>
	         		<value>LXK MPS Inbound Field</value>
				</entry>  
				<entry>
	         		<key><value>coordinatesXPreDebriefRFV</value></key>
	         		<value>LXK LBS Grid Coordinates X PreDebriefRFV</value>
				</entry>  
				<entry>
	         		<key><value>coordinatesYPreDebriefRFV</value></key>
	         		<value>LXK LBS Grid Coordinates Y PreDebriefRFV</value>
				</entry>  
			</map>
		</property>
    </bean>  
</beans>