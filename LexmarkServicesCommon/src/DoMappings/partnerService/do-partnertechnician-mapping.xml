<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="PartnerTechnicianDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
   	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       <property name="dsTargetObjectName">
			<value>LXK SW Technician List Partner Portal</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK SW Employee - Partner Portal</value>
        </property>
        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
             
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>firstName</value></key>
	         		<value>First Name</value>
				</entry>
				<entry>
	         		<key><value>lastName</value></key>
	         		<value>Last Name</value>
				</entry>
				<entry>
	         		<key><value>id</value></key>
	         		<value>Id</value>
				</entry>
				<entry>
	         		<key><value>employeeOrgId</value></key>
	         		<value>Employee Organization Id</value>
				</entry>																												
	     	</map>
		</property>
    </bean>  
</beans>