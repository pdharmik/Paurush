<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="PartnerAgreementDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
   	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       <property name="dsTargetObjectName">
			<value>LXK SW Agreement Partner Portal</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK SW Agreement - Partner Portal</value>
        </property>
        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
             
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>mdmLevel1AccountId</value></key>
	         		<value>LXK SW Agreement Account Global DUNS Number</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel2AccountId</value></key>
	         		<value>LXK SW Agreement Account Domestic DUNS Number</value>
				</entry> 
				<entry>
	         		<key><value>mdmLevel3AccountId</value></key>
	         		<value>LXK SW Agreement Account MDM Id</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel4AccountId</value></key>
	         		<value>LXK SW Agreement Account MDM Account Id</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel5AccountId</value></key>
	         		<value>LXK SW Agreement Account Id</value>
				</entry>	
				<entry>
	         		<key><value>mdmLevel</value></key>
	         		<value>LXK SW Agreement Account Level</value>
				</entry>
				<entry>
	         		<key><value>id</value></key>
	         		<value>Id</value>
				</entry> 
				<entry>
	         		<key><value>name</value></key>
	         		<value>LXK SW Agreement Name</value>
				</entry>
										
	     	</map>
		</property>
    </bean>  
</beans>