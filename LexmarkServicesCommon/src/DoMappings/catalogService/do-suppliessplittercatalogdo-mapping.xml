<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="SuppliesSplitterCatalogDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK MPS Supplies Catalog Splitter</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK MPS Supplies Catalog Splitter_LXK MPS SAP Catalog</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>

       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
          		<entry>
                     <key><value>contractNumber</value></key>
                     <value>LXK MPS Contract #</value>
                </entry>
                <entry>
                     <key><value>contractLineItemId</value></key>
                     <value>LXK MPS SAP Contract Line</value>
                </entry>
                <entry>
                     <key><value>providerContractNo</value></key>
                     <value>LXK MPS Provider Contract/Order #</value>
                </entry>
                <entry>
                     <key><value>salesOrg</value></key>
                     <value>LXK MPS SAP Sales Org</value>
                </entry>
                <entry>
                     <key><value>soldToNumber</value></key>
                     <value>LXK MPS Sold To #</value>
                </entry>
                <entry>
                     <key><value>billingModel</value></key>
                     <value>LXK MPS Billing Model</value>
                </entry>
				
	     	</map>
		</property>
	   	     	  
    </bean>  
</beans>