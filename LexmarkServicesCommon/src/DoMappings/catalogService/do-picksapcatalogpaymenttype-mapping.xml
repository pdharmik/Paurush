<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="PickSapCatalogPaymentTypeDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK MPS Hardware Catalog</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK MPS SAP Catalog - Child</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>

       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>quantity</value></key>
	         		<value>LXK MPS Quantity</value>
				</entry>
				<entry>
	         		<key><value>componentPart</value></key>
	         		<value>LXK MPS Component Part #</value>
				</entry>	
				
				<entry>
	         		<key><value>partDescription</value></key>
	         		<value>LXK MPS Part Description</value>
				</entry>
				<entry>
	         		<key><value>partType</value></key>
	         		<value>LXK MPS Part Type</value>
				</entry>
				<entry>
	         		<key><value>productType</value></key>
	         		<value>LXK MPS Product Type</value>
				</entry>	
													
				<entry>
	         		<key><value>productModel</value></key>
	         		<value>LXK MPS Product Model</value>
				</entry>			
		     </map>
		</property>
	   	     	  
    </bean>  
</beans>