<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="ProductTypeDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK MPS Supplies Catalog - Product Type_POC</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK MPS Supplies Catalog - Product Type_POC</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>

       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>id</value></key>
	         		<value>Id</value>
				</entry>
				
				<entry>
	         		<key><value>agreementId</value></key>
	         		<value>LXK MPS Agreement Id</value>
				</entry>
				<entry>
	         		<key><value>productType</value></key>
	         		<value>LXK MPS Product Type</value>
				</entry>
				<entry>
	         		<key><value>catalogProductType</value></key>
	         		<value>com.lexmark.service.impl.real.domain.SuppliesCatalogProductTypeDo</value>
				</entry>
	     	</map>
		</property>
	   	     	  
    </bean>  
</beans>