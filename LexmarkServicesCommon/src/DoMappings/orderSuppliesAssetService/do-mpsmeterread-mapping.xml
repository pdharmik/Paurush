<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="MPSMeterRead" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>Siebel BO</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK MPS Daily Meter Read BO</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK MPS Daily Meter Reads EBC</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>

       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>id</value></key>
	         		<value>Id</value>
				</entry>
				<entry>
	         		<key><value>assetId</value></key>
	         		<value>LXK MPS Asset Id</value>
				</entry>
				
                <entry>
                    <key><value>type</value></key>
                    <value>LXK MPS Meter Type</value>
                </entry>
                
                <entry>
                    <key><value>value</value></key>
                    <value>LXK MPS Meter Value</value>
                </entry>
                
                <entry>
                    <key><value>date</value></key>
                    <value>LXK MPS Meter Date</value>
                </entry>
                
	     	</map>
		</property>
	   	     	  
    </bean>  
</beans>