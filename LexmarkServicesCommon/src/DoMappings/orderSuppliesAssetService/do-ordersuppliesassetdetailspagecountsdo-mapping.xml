<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="OrderSuppliesAssetDetailsPageCountsDO" class="com.amind.data.service.DomainObjMapping">

		<!-- data source info -->
		<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
		</property>

		<property name="dsTargetObjectName">
			<value>LXK MPS Asset Detail - Meter Readings</value>
		</property>

		<property name="dsTargetComponentName">
			<value>LXK MPS Asset Detail - Service Web</value>
		</property>
		<property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
		</property>

		<!-- field mapping -->
		<property name="doFieldNameToDsFieldNameMap">
			<map>
				<entry>
					<key><value>assetId</value></key>
					<value>Asset Id</value>
				</entry>
				<entry>
					<key><value>pageCounts</value></key>
					<value>com.lexmark.service.impl.real.domain.AssetDetailsPageCountsDO</value>
				</entry>
			</map>
		</property>
	</bean>
</beans>
