<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="SoldToNumberDo" class="com.amind.data.service.DomainObjMapping">
  
		<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK MPS Agreement SoldTo IO</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK MPS SAP Sold To</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>

       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>contractNumber</value></key>
	         		<value>LXK MPS Sales Contract #</value>
				</entry>
				<entry>
	         		<key><value>soldToNumber</value></key>
	         		<value>LXK MPS Sold To #</value>
				</entry>	
		     </map>
		</property>
    </bean>  
</beans>