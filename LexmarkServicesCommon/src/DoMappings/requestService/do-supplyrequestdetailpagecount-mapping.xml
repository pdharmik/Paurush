<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="SupplyRequestDetailPageCountDo" class="com.amind.data.service.DomainObjMapping">
  
		<!-- data source info -->
      
        <property name="dataSourceName">
    		<value>EAI Siebel Adapter</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK MPS HW Service Request Detail - Service Web</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK MPS Service Request Readings BC</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
              
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>id</value></key>
	         		<value>Id</value>
				</entry>
				<entry>
	         		<key><value>name</value></key>
	         		<value>LXK MPS Measurement Name</value>
				</entry>
				<entry>
	         		<key><value>count</value></key>
	         		<value>LXK MPS Reading</value>
				</entry>
				<entry>
	         		<key><value>date</value></key>
	         		<value>LXK MPS Timestamp</value>
				</entry>
			</map>
		</property>
    </bean>  
</beans>