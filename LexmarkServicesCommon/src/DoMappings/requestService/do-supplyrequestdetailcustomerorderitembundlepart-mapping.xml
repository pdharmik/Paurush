<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="SupplyRequestDetailCustomerOrderItemBundlePartDo" class="com.amind.data.service.DomainObjMapping">
  
        <property name="dataSourceName">
    		<value>EAI Siebel Adapter</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK MPS HW Service Request Detail - Service Web</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK MPS Consumables Supplies BC - Child</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
              
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>
	     		<entry>
	         		<key><value>quantity</value></key>
	         		<value>LXK MPS Quantity</value>
				</entry>	
				<entry>
	         		<key><value>description</value></key>
	         		<value>LXK MPS HW Description</value>
				</entry>	     
				<entry>
	         		<key><value>partNumber</value></key>
	         		<value>LXK MPS Product Name</value>
				</entry>	     
			</map>
		</property>
		
    </bean>  
</beans>