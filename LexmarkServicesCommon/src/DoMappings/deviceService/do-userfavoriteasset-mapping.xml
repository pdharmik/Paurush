<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<!-- AmindDeviceService::updateUserFavoriteAsset -->
	<!-- SAMPLE QUERY: SELECT * FROM SIEBEL.S_ASSET_CON WHERE RELATION_TYPE_CD = 'Favorite' -->
	<bean id="UserFavoriteAssetDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       <property name="dsTargetObjectName">
			<value>LXK SW Contact Favorite Assets</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK SW Contact Favorite Assets</value>
        </property>
        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
             
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>contactId</value></key>
	         		<value>Contact Id</value>
				</entry>
				<entry>
	         		<key><value>favoriteAssetId</value></key>
	         		<value>Favorite Asset Id</value>
				</entry>
				<entry>
	         		<key><value>assetFavFlag</value></key>
	         		<value>LXK SW Asset Favorite Flag</value>
				</entry>
	     	</map>
		</property>
    </bean>  
</beans>