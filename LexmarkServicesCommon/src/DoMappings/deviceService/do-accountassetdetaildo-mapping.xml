        <property name="doJoinDefsMap">
			<map> 
				<entry>
					<key><value>entitlements</value></key>
					<map>
						<entry>
							<key><value>assetId</value></key>  <!-- Parent ID field -->
							<value>id</value> <!-- Child ID field-->
						</entry>    
					</map>
				</entry>
				<entry>
					<key><value>meterReads</value></key>
					<map>
						<entry>
							<key><value>assetId</value></key> 
							<value>id</value>
						</entry>    
					</map>
				</entry>
			</map>
