<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="UserFavoriteContactDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>
       
        <property name="dsTargetObjectName">
			<value>LXK SW Person Favorite Contacts</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK SW Person Favorite Contacts</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
              
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>contactId</value></key>
	         		<value>Person Id</value>
				</entry>
				<entry>
	         		<key><value>favoriteContactId</value></key>
	         		<value>Favorite Contact Id</value>
				</entry>
	     	</map>
		</property>
	   	     	  
    </bean>  
</beans>