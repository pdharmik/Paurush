<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<!-- AmindServiceRequestService::retrieveAddressList -->
	<!-- SAMPLE QUERY: SELECT X_ADDR_STATUS FROM SIEBEL.S_ADDR_PER WHERE X_ADDR_STATUS <> 'Marked For Deletion' OR X_ADDR_STATUS IS NULL -->
	<bean id="AddressDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>

        <property name="dsTargetObjectName">
			<value>LXK Service Address - Service Web</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK Service Address - Service Web</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
              
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>id</value></key>
	         		<value>Id</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel1AccountId</value></key>
	         		<value>LXK SW Account Global DUNS Number</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel2AccountId</value></key>
	         		<value>LXK SW Account Domestic DUNS Number</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel3AccountId</value></key>
	         		<value>LXK SW Account MDM Legal Entity ID #</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel4AccountId</value></key>
	         		<value>LXK SW Account MDM Account #</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel5AccountId</value></key>
	         		<value>Account Id</value>
				</entry>
				<entry>
	         		<key><value>accountTransFlag</value></key>
	         		<value>LXK SW L5 Account Transactable Flag</value>
				</entry>
				<entry>
	         		<key><value>addressName</value></key>
	         		<value>Custom Address Name</value>
				</entry>
				<entry>
	         		<key><value>addressLine1</value></key>
	         		<value>Street Address</value>
				</entry>
				<entry>
	         		<key><value>addressLine2</value></key>
	         		<value>Street Address 2</value>
				</entry>
				<entry>
	         		<key><value>addressLine3</value></key>
	         		<value>Street Address 3</value>
				</entry>
				<entry>
	         		<key><value>city</value></key>
	         		<value>City</value>
				</entry>
				<entry>
	         		<key><value>state</value></key>
	         		<value>State</value>
				</entry>
				<entry>
	         		<key><value>province</value></key>
	         		<value>Province</value>
				</entry>				
				<entry>
	         		<key><value>zip</value></key>
	         		<value>Postal Code</value>
				</entry>
				<entry>
	         		<key><value>country</value></key>
	         		<value>Country</value>
				</entry>
				<entry>
	         		<key><value>storeFrontName</value></key>
	         		<value>LXK MPS Store Front Name</value>
				</entry>
				<entry>
	         		<key><value>customAddressName</value></key>
	         		<value>Custom Address Name</value>
				</entry>
				<!--  Added by sankha for LEX:AIR00065738 start -->
				<entry>
	         		<key><value>status</value></key>
	         		<value>LXK R Account Address Status</value>
				</entry>
				<!--  End  -->
				
				<entry>
					<key><value>county</value></key>
					<value>County</value>
				</entry>
				<entry>
					<key><value>district</value></key>
					<value>District</value>
				</entry>
				<entry>
					<key><value>officeNumber</value></key>
					<value>House #</value>
				</entry>
				<entry>
					<key><value>region</value></key>
					<value>LXK Region</value>
				</entry>
				<entry>
					<key><value>countyCode</value></key>
					<value>LXK Country Code</value>
				</entry>
				<entry>
					<key><value>lbsAddressFlag</value></key>
					<value>LXK LBS Flag</value>
				</entry>
				<entry>
					<key><value>levelOfDetails</value></key>
					<value>LXK LBS Level of Detail</value>
				</entry>
	     	</map>
		</property>
	   	     	  
    </bean>  
</beans>