<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
 "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<!-- AmindServiceRequestService::retrieveAddressList -->
	<!-- SAMPLE QUERY: SELECT X_ADDR_STATUS FROM SIEBEL.S_ADDR_PER WHERE X_ADDR_STATUS <> 'Marked For Deletion' OR X_ADDR_STATUS IS NULL -->
	<bean id="LBSCHLMappingDo" class="com.amind.data.service.DomainObjMapping">
  
	<!-- data source info -->
    	<property name="dataSourceName">
			<value>EAI Siebel Adapter</value>
        </property>

        <property name="dsTargetObjectName">
			<value>LXK SW CHL</value>
        </property>
        
        <property name="dsTargetComponentName">
			<value>LXK SW CHL</value>
        </property>

        <property name="packageName">
			<value>com.lexmark.service.impl.real.domain</value>
        </property>
              
       	<!-- field mapping --> 
	   	<property name="doFieldNameToDsFieldNameMap">
	     	<map>	     
				<entry>
	         		<key><value>l5AccoountId</value></key>
	         		<value>LXK A Top Level Account Id</value>
				</entry>
				<entry>
	         		<key><value>heirarchyParentChain</value></key>
	         		<value>Parent Chain</value>
				</entry>
						
	     	</map>
		</property>
	   	     	  
    </bean>  
</beans>