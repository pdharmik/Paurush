				</entry>
			<!--	<entry>
	         		<key><value>parentChain</value></key>
	         		<value>Parent Chain</value>
				</entry> -->
				<entry>
	         		<key><value>mdmLevel1AccountId</value></key>
	         		<value>LXK SW Account Global DUNS Number</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel2AccountId</value></key>
	         		<value>LXK SW Account Domestic DUNS Number</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel3AccountId</value></key>
	         		<value>LXK SW MDM Legal Entity ID #</value>
				</entry>
				<entry>
	         		<key><value>mdmLevel4AccountId</value></key>
	         		<value>LXK SW MDM Account #</value>
				</entry> 
				<entry>
	         		<key><value>location</value></key>
	         		<value>Location</value>
				</entry> 
			<!--	<entry>
	         		<key><value>parentAccountId</value></key>
	         		<value>Parent Account Id</value> 
				</entry>
				<entry>
	         		<key><value>mdmLevel5AccountId</value></key>
	         		<value>LXK A Top Level Account Id</value>
				</entry> -->
				<entry>
	         		<key><value>chlList</value></key>
	         		<value>com.lexmark.service.impl.real.domain.CHLAccountCHLDo</value>
				</entry>
				
			</map>
		</property>
		
		<!-- join defs for contained component class refs that are from a different Data Object--> 
	   <property name="doJoinDefsMap">
	     	<map> 

 	       		<entry>
	         		<key><value>chlList</value></key>
	         		<map>
	         			<entry>
	         				<key><value>parentAccountId</value></key>
	         				<value>mdmLevel5AccountId</value>
	       	 			</entry>	
	       	 		</map>
	       		</entry>
			</map>
	   </property>
