/* added by offshore */
.requestsUploader {
	filter: alpha(opacity = 0);
	opacity: 0;
	cursor: pointer !important;
    height: 28px;
    margin: 0;
    position: relative;
    left:-20px;
    width: 113px;
    z-index: 2;
    vertical-align:top;
    padding:0!important
    }
.ie .requestsUploader {
    left:-34px;}
.ie7 .requestsUploader {
    top: 2px;
	left:-38px;}
.webkit .requestsUploader {margin:0 33px 0 0;width:80px;left:0px;}

#btnBrowse{left: -105px;
    padding: 4px 7px;
    position: relative;
    margin:0px;}
.ie7 #btnBrowse{margin:0 2px 0 0;}

#btnUpload{	left: -105px;
    position: relative;
	padding: 4px 7px;}
    
@-moz-document url-prefix() {
	.requestsUploader {left:0px;}
	#btnBrowse{padding: 7px 15px; }
	#btnUpload{padding: 7px 15px; }
}
