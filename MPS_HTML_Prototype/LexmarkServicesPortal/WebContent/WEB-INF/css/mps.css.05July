/* CART */
.cart {
	width:150px;
	margin:3px 5px 2px 0;
	float:right
}
.cart a {
	display:block;
	background:#fff url(/LexmarkServicesPortal/images/cart.png) no-repeat 5px 2px;
	padding:5px 5px 5px 40px;
	color:#666;
	font-size:14px;
	text-decoration:none;
	font-weight:bold;
}
.cart a:hover, .cart a:active {
	text-decoration:none;
	color:#999;
	font-weight:bold!important
}
.cart a span {
	color:#000;
	font-size:16px;
	display:inline-block;
	padding:0 5px
}
/* PROCESS STEPS */
ul.processSteps {
	border: solid 4px #fff;
	-webkit-border-radius: .5em;
	-moz-border-radius: .5em;
	border-radius: .5em;
	background: #f7f7f7;
	padding: 0 5px;
	overflow:hidden;
	font-family:Arial, Helvetica, sans-serif;
	font-size:16px;
	font-weight:bold;
	display:inline-block;
	zoom:1;
	width:auto;
	margin: 10px 0 5px 0;
}
ul.processSteps li {
	display:inline;
	float:left;
	background:url(/LexmarkServicesPortal/images/stepsPoint.png) no-repeat right center;
}
ul.processSteps li a {
	display:inline-block;
	padding:18px 28px 18px 0px;
	background:url(/LexmarkServicesPortal/images/steps_bg_disabled.png) no-repeat 5px 6px;
	color:#b5b5b5;
	cursor:default;
	text-decoration: none!important
	vertical-align:baseline;
}
ul.processSteps li a:hover { text-decoration: none }
ul.processSteps li a:active { font-weight:bold!important }
ul.processSteps li a span {
	display:inline-block;
	float:left;
	font-size:28px;
	width:52px;
	margin-right:5px;
	text-align:center
}
ul.processSteps li.active a {
	color:#3a7fa8;
	background-image:url(/LexmarkServicesPortal/images/steps_bg_active.png);
	cursor:pointer
}
ul.processSteps li.selected a {
	color:#339933;
	background-image:url(/LexmarkServicesPortal/images/steps_bg_select.png)
}
ul.processSteps li.last { background-image:none }
ul.processSteps li.last a { padding-right:5px }
/* FORM ELEMENTS */
ul.form {
	float:left;
	width:100%;
	margin-bottom:5px
}
ul.form li {
	background:none;
	display:block;
	clear:both;
	list-style-type:none!important;
	line-height:2.2em;
	padding-left:44%;
}
ul.form li label {
	display:inline-block;
	float:left;
	padding:2px 0 2px 0;
